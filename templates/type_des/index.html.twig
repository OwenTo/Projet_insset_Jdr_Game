{% extends 'base.html.twig' %}

{% block title %}TypeDes index{% endblock %}

{% block body %}
    <h1>TypeDes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Des</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_de in type_des %}
            <tr>
                <td>{{ type_de.id }}</td>
                <td>{{ type_de.des }}</td>
                <td>
                    <a href="{{ path('type_des_show', {'id': type_de.id}) }}">show</a>
                    <a href="{{ path('type_des_edit', {'id': type_de.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_des_new') }}">Create new</a>
{% endblock %}
