{% extends 'base.html.twig' %}

{% block title %}Fichier{% endblock %}

{% block body %}
    <h1>Fichier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fichier.id }}</td>
            </tr>
            <tr>
                <th>ContenueFichier</th>
                <td>{{ fichier.contenueFichier }}</td>
            </tr>
            <tr>
                <th>FichierExtension</th>
                <td>{{ fichier.fichierExtension }}</td>
            </tr>
            <tr>
                <th>CreateFileAt</th>
                <td>{{ fichier.createFileAt ? fichier.createFileAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ModifFileAt</th>
                <td>{{ fichier.modifFileAt ? fichier.modifFileAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('fichier_index',{'idUser':fichier.user.id}) }}">back to list</a>

    <a href="{{ path('fichier_edit', {'idUser':fichier.user.id,'id': fichier.id}) }}">edit</a>

    {{ include('fichier/_delete_form.html.twig') }}
{% endblock %}
