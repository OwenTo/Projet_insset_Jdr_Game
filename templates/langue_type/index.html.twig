{% extends 'base.html.twig' %}

{% block title %}LangueType index{% endblock %}

{% block body %}
    <h1>LangueType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>LangueType</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for langue_type in langue_types %}
            <tr>
                <td>{{ langue_type.id }}</td>
                <td>{{ langue_type.langueType }}</td>
                <td>
                    <a href="{{ path('langue_type_show', {'id': langue_type.id}) }}">show</a>
                    <a href="{{ path('langue_type_edit', {'id': langue_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('langue_type_new') }}">Create new</a>
{% endblock %}
